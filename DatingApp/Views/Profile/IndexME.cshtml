@model DatingApp.Models.ProfileViewModels.ProfileIndexViewModel

<h2>This is your profile</h2>

<img src="@Url.Content(Model.Image)" alt="Image" class="profile-image" />
<br />
@Html.LabelFor(m => m.Name, htmlAttributes: new { @class = "control-label col-md-2" })
<br />
@Html.DisplayFor((m) => m.Name)
<br />
@Html.LabelFor(m => m.Age, htmlAttributes: new { @class = "control-label col-md-2" })
<br />
@Html.DisplayFor((m) => m.Age)
<br />
@Html.LabelFor(m => m._Gender, htmlAttributes: new { @class = "control-label col-md-2" })
<br />
@Html.DisplayFor((m) => m._Gender)
<br />
@Html.LabelFor(m => m.Biography, htmlAttributes: new { @class = "control-label col-md-2" })
<br />
@Html.DisplayFor((m) => m.Biography)
<br />
<input type="button"
       value="Edit"
       class="btn btn-default"
       onclick="location.href='@Url.Action("Edit", "Profile" )'" />